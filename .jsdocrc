{
	"opts": {
		"destination": "./docs/",
		"recurse": true
	},
	"source": {
		"includePattern": "src/.*js"
	},
	"tags": {
		"allowUnknownTags": false
	},
	"templates": {
		"collapseSymbols": true,
		"includeDate": false,
		"linenums": true,
		"outputSourceFiles": true,
		"syntaxTheme": "dark",
		"systemName": "semantic-release-gitlab",
		"theme": "slate"
	}
}
